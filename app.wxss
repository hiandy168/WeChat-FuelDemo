/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.index-bg{
  display: flex;
  align-items: center;
  flex-direction: column;
  width: 100%;
  height: 100%;
  background-image: url("../../images/bg.png");
}
.home-title-image{
  display: flex;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
  flex-direction: column;
  align-items: center;
  width: 400rpx;
  height: 100rpx;
}
.home-rect{
  display: flex;
  flex-direction: column;
  align-items: center;
  align-content: center;
  margin-right: 75rpx;
  margin-left: 75rpx;
  border: solid 2px #ffffff;
}
.home-view-item{
  display: flex;
  flex-direction: row;
  align-items: center;
  align-content: center;
}
.home-rect-child-view{
   display: flex;
  flex-direction: column;
  justify-content: center;
  width: 300rpx;
  height: 100rpx;
}
.rect-vertical-line{
  height: 100rpx;
  width: 4rpx;
  background: #ffffff;
}
.home-text{
  color: #000;
  text-align: center;
  font-size: 14px;
}
.rect-horizontal-line{
  height: 4rpx;
  width: 100%;
  background: #fff;
}
.home-button-view{
  display: flex;
  flex-direction: row;
  margin-top: 50rpx;
  margin-bottom: 50rpx;
  margin-left: 100rpx;
  margin-right: 100rpx;
}
.home-button{
  display: flex;
  flex-direction: row;
  width: 120rpx;
  height: 60rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  align-items: center;
  justify-content: center;
  border: solid 1px #157efb;
  border-radius: 6px;
}
.home-button-text{
  font-size: 14px;
  color: #157efb;
  text-align: center
}
